  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0 --port 80",
    "lint": "eslint ./",
    "lint:auto": "eslint ./ --fix",
